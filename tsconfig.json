{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/(auth)/sign-up.js",
    "app/(auth)/index.js",
    "app/(auth)/_layout.js",
    "app/(tabs)/index.js",
    "app/(tabs)/collections.js",
    "app/(tabs)/_layout.js",
    "app/_layout.js"
  ]
}
