{
  "expo": {
    "name": "Taiar",
    "slug": "taiar",
    "version": "1.8.0",
    "orientation": "portrait",
    "icon": "./assets/images/tayar_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "workflow": "managed",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      },
      "bundleIdentifier": "com.expresstaiar.taiar",
      "userInterfaceStyle": "automatic",
      "config": {
        "isRTL": true
      },
      "supportsRTL": true
    },
    "android": {
      "package": "com.taiar",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tayar_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.RESTART_PACKAGES",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "NOTIFICATIONS"
      ],
      "userInterfaceStyle": "automatic",
      "allowBackup": false,
      "config": {
        "isRTL": true
      },
      "supportsRTL": true,
      "jsEngine": "hermes",
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tayar_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tayar_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow app to use Face ID for authentication"
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/tayar_logo.png",
          "color": "#ffffff",
          "sounds": [],
          "androidMode": "default",
          "androidCollapsedTitle": "Updates from Taiar",
          "iosDisplayInForeground": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.9.25"
          }
        }
      ],
      "./app.plugin.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c1d54568-437f-4a8c-a692-8b8ac589ffb0"
      }
    },
    "owner": "expresstaiar",
    "notification": {
      "icon": "./assets/images/tayar_logo.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Taiar",
      "iosDisplayInForeground": true
    }
  }
}
