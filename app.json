{
  "expo": {
    "name": "Taiar",
    "slug": "taiar",
    "version": "2.4.3",
    "orientation": "portrait",
    "icon": "./assets/images/tayar_logo_dark.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c1d54568-437f-4a8c-a692-8b8ac589ffb0",
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "ios": {
      "workflow": "managed",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      },
      "bundleIdentifier": "com.expresstaiar.taiar",
      "userInterfaceStyle": "automatic",
      "config": {
        "isRTL": true
      },
      "supportsRTL": true
    },
    "android": {
  "package": "com.taiar",
  "googleServicesFile": "./google-services.json",
  "adaptiveIcon": {
    "foregroundImage": "./assets/images/tayar_logo_dark.png",
    "backgroundColor": "#252424"
  },
  "permissions": [
    "android.permission.CAMERA",
    "android.permission.RECORD_AUDIO",
    "android.permission.USE_BIOMETRIC",
    "android.permission.USE_FINGERPRINT",
    "android.permission.RESTART_PACKAGES",
    "android.permission.RECEIVE_BOOT_COMPLETED",
    "android.permission.VIBRATE",
    "android.permission.WAKE_LOCK",
    "NOTIFICATIONS"
  ],
  "userInterfaceStyle": "automatic",
  "allowBackup": false,
  "config": {
    "isRTL": true
  },
  "supportsRTL": true,
  "jsEngine": "hermes",
  "useNextNotificationsApi": true
},
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tayar_logo_dark.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tayar_logo_dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#252424"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow app to use Face ID for authentication"
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/tayar_logo_dark.png",
          "color": "#252424",
          "sounds": [],
          "androidMode": "default",
          "androidCollapsedTitle": "Updates from Taiar",
          "iosDisplayInForeground": true,
          "enableBackgroundRemoteNotifications": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "1.9.25"
          }
        }
      ],
      "./app.plugin.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c1d54568-437f-4a8c-a692-8b8ac589ffb0"
      }
    },
    "owner": "expresstaiar",
    "notification": {
      "icon": "./assets/images/tayar_logo_dark.png",
      "color": "#252424",
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Taiar",
      "iosDisplayInForeground": true
    }
  }
}